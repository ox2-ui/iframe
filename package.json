{
  "name": "@ox2/iframe",
  "version": "0.0.3",
  "description": "Auto sizable iframe component.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ox2/iframe"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files",
    "build-storybook": "build-storybook -s ./public",
    "build:babel": "babel ./src --ignore *.story.js,*.test.js --out-dir ./build",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "docs": "styleguidist server",
    "docs:build": "styleguidist build && npm run docs:copy-to-public",
    "docs:copy-to-public": "bash ./scripts/copy_docs.sh",
    "lint": "eslint src scripts && echo \"eslint: NO LINT ERRORS\"; exit 0",
    "prebuild": "npm run clean:build && npm run lint && npm run test",
    "prettier": "prettier --print-width 60 --single-quote --trailing-comma all --write \"{src,.storybook,scripts}/**/*.js\"",
    "storybook": "start-storybook -p 55001 -s ./public",
    "test": "jest",
    "test:update": "yarn test -- -u"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.1",
    "@kadira/storybook-addon-options": "^1.0.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.12.2",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^1.0.2",
    "eslint-plugin-react": "^6.7.1",
    "fs-extra": "^1.0.0",
    "jest-cli": "^18.1.0",
    "prettier": "^1.3.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-styleguidist": "^4.4.3",
    "rimraf": "^2.5.3"
  },
  "main": "./index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react-virtualized": "^9.7.6"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "jest": {
    "testPathDirs": [
      "./src"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
